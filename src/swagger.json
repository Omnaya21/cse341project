  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "displayName": {
            "type": "string",
            "example": "Mike Johnson"
          }
        },
        "required": [
          "displayName"
        ]
      },
      "Tag": {
        "type": "object",
        "properties": {
          "tag": {
            "type": "string",
            "example": "Outdoors"
          },
          "createdAt": {
            "type": "string",
            "example": "2023-07-01T01:33:43.197Z"
          }
        },
        "required": [
          "tag"
        ]
      },
      "Comment": {
        "type": "object",
        "properties": {
          "comment": {
            "type": "string",
            "example": "This was great info. Thank you!"
          },
          "author": {
            "$ref": "#/components/schemas/User"
          },
          "createdAt": {
            "type": "string",
            "example": "2023-06-24T01:33:43.197Z"
          },
          "updatedAt": {
            "type": "string",
            "example": ""
          }
        },
        "required": [
          "comment"
        ]
      },
      "Post": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "3 Day Backpacking List"
          },
          "author": {
            "$ref": "#/components/schemas/User"
          },
          "content": {
            "type": "string",
            "example": "In this post, I'm going to go over the best things to pack for a 3 day backpacking trip in southern Utah..."
          },
          "createdAt": {
            "type": "string",
            "example": "2023-06-23T01:33:43.197Z"
          },
          "updatedAt": {
            "type": "string",
            "example": "2023-06-23T02:05:43.197Z"
          },
          "image": {
            "type": "string",
            "example": ""
          },
          "comments": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Comment"
            }
          },
          "tags": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tag"
            }
          }
        },
        "required": [
          "title",
          "author",
          "content"
        ]
      }
    }
  }
